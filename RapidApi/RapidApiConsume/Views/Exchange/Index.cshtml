@model List<ExchangeViewModel.Exchange_Rates>

@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /><br />
    <table class="table table-bordered">
        <tr>
            <th>Döviz Kodu</th>
            <th>Birim Fiyat</th>
        </tr>
        @foreach (var exchangeView in Model)
        {
             <tr>
                <td>@exchangeView.currency</td>
                <td>@exchangeView.exchange_rate_buy</td>
             </tr>   
        }
    </table>
</div>
