@model List<ResultAboutDto>

@{
    ViewBag.Title = "Çalışanlarımız";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık 1</th>
        <th>Başlık 2</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var about in Model)
    {
        <tr>
            <th>@about.Id</th>
            <td>@about.Title1</td>
            <td>@about.Title2</td>
            <td><a href="/AdminAbout/UpdateAbout/@about.Id" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
        </tr>
    }
</table>

